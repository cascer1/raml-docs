{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "Global definitions for error responses calls",
  "definitions" : {
    "APIError" : {
      "type" : "object",
      "properties" : {
        "code" : {
          "type" : "string"
        },
        "httpStatusCode" : {
          "type" : "integer",
          "maximum" : 2147483647
        },
        "message" : {
          "type" : "string"
        },
        "propertyName" : {
          "type" : "string"
        },
        "requestId" : {
          "type" : "string"
        }
      },
      "required" : [ "code" ]
    }
  }
}