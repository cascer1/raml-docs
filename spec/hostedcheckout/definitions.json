{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "Global definitions for hosted checkout calls",
  "definitions" : {
    "CreatedPaymentOutput" : {
      "type" : "object",
      "properties" : {
        "displayedData" : {
          "$ref" : "#/definitions/DisplayedData"
        },
        "payment" : {
          "$ref" : "../payment/definitions.json#/definitions/Payment"
        },
        "paymentCreationReferences" : {
          "$ref" : "../payment/definitions.json#/definitions/PaymentCreationReferences"
        },
        "paymentStatusCategory" : {
          "$ref" : "../payment/definitions.json#/definitions/PaymentStatusCategory"
        },
        "tokens" : {
          "type" : "string"
        }
      }
    },
    "DisplayedData" : {
      "type" : "object",
      "properties" : {
        "displayedDataType" : {
          "$ref" : "../payment/definitions.json#/definitions/MerchantActionType"
        },
        "renderingData" : {
          "type" : "string"
        },
        "showData" : {
          "type" : "array",
          "items" : {
            "$ref" : "../definitions.json#/definitions/KeyValuePair"
          },
          "minItems" : 0,
          "uniqueItems" : false
        }
      }
    },
    "HostedCheckoutSpecificInput" : {
      "type" : "object",
      "properties" : {
        "isRecurring" : {
          "type" : "boolean"
        },
        "locale" : {
          "type" : "string"
        },
        "paymentProductFilters" : {
          "$ref" : "#/definitions/PaymentProductFiltersHostedCheckout"
        },
        "returnUrl" : {
          "type" : "string"
        },
        "showResultPage" : {
          "type" : "boolean"
        },
        "tokens" : {
          "type" : "string"
        },
        "variant" : {
          "type" : "string"
        }
      }
    },
    "HostedCheckoutStatus" : {
      "type" : "string",
      "enum" : [ "IN_PROGRESS", "PAYMENT_CREATED" ]
    },
    "PaymentProductFiltersHostedCheckout" : {
      "type" : "object",
      "properties" : {
        "exclude" : {
          "$ref" : "../definitions.json#/definitions/PaymentProductFilter"
        },
        "restrictTo" : {
          "$ref" : "../definitions.json#/definitions/PaymentProductFilter"
        },
        "tokensOnly" : {
          "type" : "boolean"
        }
      }
    }
  }
}